    <script>
        // Force ASQ book background color
        document.addEventListener('DOMContentLoaded', function() {
            // Force ASQ book background - try multiple times
            function forceASQBackground() {
                const asqBook = document.querySelector('#books-component-container-full [data-book-title="ASQ Certification"] .books__image');
                const asqBookLast = document.querySelector('#books-component-container-full .books-wrapper .books__item:last-child .books__image');
                const asqBookImg = document.querySelector('#books-component-container-full .books-wrapper .books__item:last-child .books__image img');
                
                if (asqBook) {
                    asqBook.style.backgroundColor = '#2c3e50';
                    asqBook.style.setProperty('background-color', '#2c3e50', 'important');
                    asqBook.style.background = '#2c3e50';
                    asqBook.style.setProperty('background', '#2c3e50', 'important');
                }
                
                if (asqBookLast) {
                    asqBookLast.style.backgroundColor = '#2c3e50';
                    asqBookLast.style.setProperty('background-color', '#2c3e50', 'important');
                    asqBookLast.style.background = '#2c3e50';
                    asqBookLast.style.setProperty('background', '#2c3e50', 'important');
                }
                
                if (asqBookImg) {
                    asqBookImg.style.backgroundColor = '#2c3e50';
                    asqBookImg.style.setProperty('background-color', '#2c3e50', 'important');
                    asqBookImg.style.background = '#2c3e50';
                    asqBookImg.style.setProperty('background', '#2c3e50', 'important');
                }
            }
            
            // Run immediately and after a delay
            forceASQBackground();
            setTimeout(forceASQBackground, 100);
            setTimeout(forceASQBackground, 500);
            setTimeout(forceASQBackground, 1000);
        });
        
        // Accordion functionality
        document.addEventListener('DOMContentLoaded', function() {
            const accordionHeaders = document.querySelectorAll('.accordion-header');
            
            accordionHeaders.forEach(header => {
                header.addEventListener('click', function() {
                    const accordionItem = this.parentElement;
                    const accordionContent = accordionItem.querySelector('.accordion-content');
                    const isActive = accordionItem.classList.contains('active');
                    
                    // Close all other accordion items
                    document.querySelectorAll('.accordion-item').forEach(item => {
                        item.classList.remove('active');
                        item.querySelector('.accordion-content').classList.remove('active');
                    });
                    
                    // Toggle current item
                    if (!isActive) {
                        accordionItem.classList.add('active');
                        accordionContent.classList.add('active');
                    }
                });
            });
        });
